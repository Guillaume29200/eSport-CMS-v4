# eSport-CMS V4 - Configuration
# Copier ce fichier en ".env" et modifier les valeurs

# Environnement (development, staging, production)
APP_ENV=development
APP_URL=http://localhost
APP_TIMEZONE=Europe/Paris

# Base de données
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_NAME=v4
DB_USER=root
DB_PASS=

# Sécurité (générer avec: openssl rand -hex 32)
CSRF_SECRET=your_csrf_secret_here
SESSION_SECRET=your_session_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Modules activés (1=activé, 0=désactivé)
MODULE_PLUMEAI=1
MODULE_GSHQ=0
MODULE_MULTIAI=1

# API Keys (optionnel)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
MISTRAL_API_KEY=
